{
    "name": "vscode-rrebnf",
    "displayName": "EBNF HighLighting",
    "description": "EBNF HighLighting RRDG grammar",
    "version": "0.0.3",
    "publisher": "VShilenkov",
    "engines": {
        "vscode": "^1.63.0"
    },
    "categories": [
        "Programming Languages"
    ],
    "activationEvents": [
        "onLanguage:ebnf"
    ],
    "main": "./out/extension.js",
    "repository": {
        "type": "git",
        "url": "https://github.com/VShilenkov/vscode-rrebnf"
    },
    "homepage": "https://github.com/VShilenkov/vscode-rrebnf",
    "bugs": {
        "url": "https://github.com/VShilenkov/vscode-rrebnf/issues"
    },
    "keywords": [
        "syntax-highlighting",
        "vscode",
        "vscode-extension",
        "highlight",
        "vscode-language",
        "railroad-diagrams",
        "ebnf-syntax"
    ],
    "contributes": {
        "languages": [
            {
                "id": "ebnf",
                "aliases": [
                    "EBNF",
                    "ebnf"
                ],
                "extensions": [
                    ".ebnf"
                ],
                "configuration": "./language-configuration.json"
            }
        ],
        "grammars": [
            {
                "language": "ebnf",
                "scopeName": "source.ebnf",
                "path": "./syntaxes/ebnf.tmLanguage.json"
            }
        ]
    },
    "scripts": {
        "vscode:prepublish": "npm run compile",
        "compile": "tsc -p ./",
        "watch": "tsc -watch -p ./",
        "pretest": "npm run compile && npm run lint",
        "lint": "eslint src --ext ts"
    },
    "devDependencies": {
        "@types/vscode": "^1.63.0",
        "@types/glob": "^7.1.4",
        "@types/mocha": "^9.0.0",
        "@types/node": "14.x",
        "@typescript-eslint/eslint-plugin": "^5.1.0",
        "@typescript-eslint/parser": "^5.1.0",
        "eslint": "^8.1.0",
        "glob": "^7.1.7",
        "mocha": "^9.1.3",
        "typescript": "^4.4.4",
        "@vscode/test-electron": "^1.6.2"
    },
    "extensionDependencies": [
        "draivin.hscopes"
    ]
}